{
    "manifest_version": 1,
  
    "name": "Monochrome",
    "description": "Chrome extension that turns web pages into grayscale.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ]
  }
  